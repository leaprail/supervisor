[<%= @eventlistener ? 'eventlistener' : 'program' %>:<%= @process_name %>]
command=<%= @command %>
process_name=<%= @process_name %>
numprocs=<%= @numprocs %>
numprocs_start=<%= @numprocs_start %>
priority=<%= @priority %>
autostart=<%= @autostart ? 'true' : 'false' %>
autorestart=<%= @autorestart.is_a?(String) ? @autorestart : ( @autorestart ? 'true' : 'false' ) %>
startsecs=<%= @startsecs %>
startretries=<%= @startretries %>
exitcodes=<%= @exitcodes.join(',') %>
stopsignal=<%= @stopsignal %>
stopwaitsecs=<%= @stopwaitsecs %>
<% unless @stopasgroup.nil? %>
stopasgroup=<%= @stopasgroup %>
<% end %>
<% unless @killasgroup.nil? %>
killasgroup=<%= @killasgroup %>
<% end %>
<% if @user %>
user=<%= @user %>
<% end %>
redirect_stderr=<%= @redirect_stderr ? 'true' : 'false' %>
stdout_logfile=<%= @stdout_logfile %>
stdout_logfile_maxbytes=<%= @stdout_logfile_maxbytes %>
stdout_logfile_backups=<%= @stdout_logfile_backups %>
stdout_capture_maxbytes=<%= @stdout_capture_maxbytes %>
stdout_events_enabled=<%= @stdout_events_enabled ? 'true' : 'false' %>
stderr_logfile=<%= @stderr_logfile %>
stderr_logfile_maxbytes=<%= @stderr_logfile_maxbytes %>
stderr_logfile_backups=<%= @stderr_logfile_backups %>
stderr_capture_maxbytes=<%= @stderr_capture_maxbytes %>
stderr_events_enabled=<%= @stderr_events_enabled ? 'true' : 'false' %>
<% unless @environment.empty? %>
environment=<%= @environment.sort.map{|k,v| "#{k}=\"#{v}\""}.join(',') %>
<% end %>
<% if @directory %>
directory=<%= @directory %>
<% end %>
<% if @umask %>
umask=<%= @umask %>
<% end %>
serverurl=<%= @serverurl %>
<% unless @eventlistener_events.nil? or @eventlistener_events.empty? %>
events=<%= @eventlistener_events.join(',') %>
<% end %>
<% unless @eventlistener_buffer_size.nil? %>
buffer_size=<%= @eventlistener_buffer_size %>
<% end %>
